def duplicate_encode(word):
    normalized_word = word.lower()
    
    char_count = {}
    for char in normalized_word:
        char_count[char] = char_count.get(char, 0) + 1
    
    encoded_string = ''
    for char in normalized_word:
        if char_count[char] == 1:
            encoded_string += '('
        else:
            encoded_string += ')'
    
    return encoded_string