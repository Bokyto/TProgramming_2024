class Node:
    def __init__(self, data):
        self.data = data
        self.next = None

def sorted_insert(head, data):
    if head is None:
        return Node(data)
    if data < head.data:
        new_head = Node(data)
        new_head.next = head
        return new_head
    current_node = head
    while current_node.next is not None and current_node.next.data < data:
        current_node = current_node.next
    insert = Node(data)
    insert.next = current_node.next
    current_node.next = insert
    return head